{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "Dashboard",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack/dev.config.js --host 0.0.0.0 --port 3000 --content-base public/ --history-api-fallback --hot --inline --colors",
    "clean": "rm -rf ./dist",
    "check": "npm run styles && npm run eslint",
    "compile": "mkdir -p ./dist && rsync -av ./public/* ./dist/ --exclude index.html && webpack --config webpack/production.config.js",
    "build": "npm run check && npm run clean && npm run test && npm run compile",
    "build:serve": "static dist",
    "test": "mocha --require ./test/test_helper.js 'test/**/*-test.js'",
    "styles": "eclint check './src/**/*.js'",
    "eslint": "eslint src",
    "eslint:fix": "eslint --fix src",
    "eslint:config": "eslint --print-config src",
    "uninstall": "npm uninstall `ls -1 node_modules | tr '/\n' ' '`"
  },
  "repository": {
    "type": "git",
    "url": "git+https://vgurnani-systango@bitbucket.org/vgurnani-systango/react-starter-kit.git"
  },
  "author": "",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "devDependencies": {
    "animate.css": "^3.4.0",
    "autoprefixer": "^6.3.1",
    "babel-core": "^6.1.21",
    "babel-loader": "^6.1.0",
    "babel-polyfill": "^6.1.19",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "babel-register": "^6.9.0",
    "bowser": "^1.0.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.2.0",
    "classnames": "^2.2.0",
    "css-loader": "^0.23.0",
    "eclint": "^1.1.5",
    "eslint": "^2.2.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-standard": "^1.3.2",
    "file-loader": "^0.8.5",
    "fingerprintjs2": "^1.3.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "isomorphic-fetch": "^2.2.0",
    "jsonwebtoken": "^5.5.4",
    "jws": "^3.1.0",
    "lodash": "^4.0.0",
    "mocha": "^2.3.4",
    "moment": "^2.13.0",
    "nock": "^8.0.0",
    "node-static": "^0.7.7",
    "normalizr": "^2.0.0",
    "platform": "^1.3.1",
    "postcss-loader": "^0.9.1",
    "qs": "^6.0.1",
    "react": "^15.0.0",
    "react-addons-css-transition-group": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-hot-loader": "^1.3.0",
    "react-input-mask": "^0.5.10",
    "react-redux": "^4.0.0",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "react-time-picker": "^1.1.0",
    "redbox-react": "^1.2.2",
    "redux": "^3.0.4",
    "redux-form": "^5.2.3",
    "redux-logger": "^2.0.4",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.1",
    "sha1": "^1.1.1",
    "sinon": "^1.17.4",
